{
  "$schema": "http://json-schema.org/draft-06/schema",
  "type": "object",
  "properties": {
    "namelist": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "infolist": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "filename": {
            "type": "string"
          },
          "date_time": {
            "type": "integer"
          },
          "compress_type": {
            "type": "integer"
          },
          "comment": {
            "type": "string"
          },
          "create_system": {
            "type": "integer"
          },
          "create_version": {
            "type": "integer"
          },
          "extract_version": {
            "type": "integer"
          },
          "flag_bits": {
            "type": "integer"
          },
          "volume": {
            "type": "integer"
          },
          "internal_attr": {
            "type": "integer"
          },
          "external_attr": {
            "type": "integer"
          },
          "header_offset": {
            "type": "integer"
          },
          "CRC": {
            "type": "integer"
          },
          "compress_size": {
            "type": "integer"
          },
          "file_size": {
            "type": "integer"
          }
        }
      }
    }
  }
}