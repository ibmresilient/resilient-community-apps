_str(playbook.inputs.bmc_helix_service_type),\\n  mk_str(playbook.inputs.bmc_helix_status),\\n  mk_str(playbook.inputs.bmc_helix_reported_source),\\n  mk_str(playbook.inputs.bmc_helix_note),\\n  mk_str(playbook.inputs.bmc_helix_support_group),\\n  playbook.inputs.bmc_helix_additional_data.content if playbook.inputs.bmc_helix_additional_data.content else \\\"null\\\"\\n)\\n\\n# set inputs\\ninputs.incident_id = incident.id\\ninputs.helix_incident_name = incident.name\\ninputs.helix_payload = payload\",\"pre_processing_script_language\":\"python3\",\"result_name\":\"created_incident\"}\u003c/resilient:function\u003e\u003c/extensionElements\u003e\u003cincoming\u003eFlow_0s21wv9\u003c/incoming\u003e\u003coutgoing\u003eFlow_0kw4l7s\u003c/outgoing\u003e\u003c/serviceTask\u003e\u003csequenceFlow id=\"Flow_0s21wv9\" sourceRef=\"StartEvent_155asxm\" targetRef=\"ServiceTask_1\"/\u003e\u003cscriptTask id=\"ScriptTask_2\" name=\"post-process\"\u003e\u003cextensionElements\u003e\u003cresilient:script uuid=\"333fbf2a-7f67-46b7-8c96-20c2e54b2008\"/\u003e\u003c/extensionElements\u003e\u003cincoming\u003eFlow_0kw4l7s\u003c/incoming\u003e\u003coutgoing\u003eFlow_00x0vp6\u003c/outgoing\u003e\u003cscript\u003escript\u003c/script\u003e\u003c/scriptTask\u003e\u003csequenceFlow id=\"Flow_0kw4l7s\" sourceRef=\"ServiceTask_1\" targetRef=\"ScriptTask_2\"/\u003e\u003cendEvent id=\"EndPoint_3\" resilient:documentation=\"End point\"\u003e\u003cincoming\u003eFlow_00x0vp6\u003c/incoming\u003e\u003c/endEvent\u003e\u003csequenceFlow id=\"Flow_00x0vp6\" sourceRef=\"ScriptTask_2\" targetRef=\"EndPoint_3\"/\u003e\u003c/process\u003e\u003cbpmndi:BPMNDiagram id=\"BPMNDiagram_1\"\u003e\u003cbpmndi:BPMNPlane bpmnElement=\"playbook_7c7a5fbc_eba2_4b90_b1fa_a6ba9c4e3b72\" id=\"BPMNPlane_1\"\u003e\u003cbpmndi:BPMNEdge bpmnElement=\"Flow_00x0vp6\" id=\"Flow_00x0vp6_di\"\u003e\u003comgdi:waypoint x=\"721\" y=\"432\"/\u003e\u003comgdi:waypoint x=\"721\" y=\"524\"/\u003e\u003c/bpmndi:BPMNEdge\u003e\u003cbpmndi:BP