      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "context": {
            "type": "array"
          },
          "created": {
            "type": "string"
          },
          "description": {},
          "human_url": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "last_analyses": {
            "type": "object"
          },
          "sources": {
            "type": "array"
          },
          "tags": {
            "type": "array",
