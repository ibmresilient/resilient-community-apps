{
  "$schema": "http://json-schema.org/draft-06/schema",
  "type": "object",
  "properties": {
    "version": {
      "type": "string"
    },
    "success": {
      "type": "boolean"
    },
    "reason": {},
    "content": {
      "type": "object",
      "properties": {
        "mitre_techniques": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "description": {
                "type": "string"
              },
              "external_references": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "url": {
                      "type": "string"
                    }
                  }
                }
              },
              "x_mitre_detection": {
                "type": "string"
              },
              "id": {
                "type": "string"
              },
              "collection": {
                "type": "string"
              },
              "tactic": {
                "type": "string"
              },
              "mitre_mitigations": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "description": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "id": {
                      "type": "string"
                    },
                    "collection": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "raw": {
      "type": "string"
    },
    "inputs": {
      "type": "object",
      "properties": {
        "mitre_technique_name": {
          "type": "string"
        },
        "mitre_technique_id": {
          "type": "string"
        },
        "mitre_technique_mitigation_only": {
          "type": "boolean"
        }
      }
    },
    "metrics": {
      "type": "object",
      "properties": {
        "version": {
          "type": "string"
        },
        "package": {
          "type": "string"
        },
        "package_version": {
          "type": "string"
        },
        "host": {
          "type": "string"
        },
        "execution_time_ms": {
          "type": "integer"
        },
        "timestamp": {
          "type": "string"
        }
      }
    }
  }
}