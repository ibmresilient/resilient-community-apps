{
  "$schema": "http://json-schema.org/draft-06/schema",
  "type": "object",
  "properties": {
    "version": {
      "type": "number"
    },
    "success": {
      "type": "boolean"
    },
    "reason": {},
    "content": {
      "type": "object",
      "properties": {
        "state": {
          "type": "string"
        },
        "summary": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "owner": {
          "type": "string"
        },
        "team": {
          "type": "string"
        },
        "closedOwner": {
          "type": "string"
        },
        "closedTeam": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "displayId": {
          "type": "string"
        },
        "createdTime": {
          "type": "string"
        },
        "lastChanged": {
          "type": "string"
        },
        "firstInProgressTime": {
          "type": "string"
        },
        "resolvedTime": {
          "type": "string"
        },
        "eventTypeCounts": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "eventType": {
                "type": "string"
              },
              "count": {
                "type": "integer"
              }
            }
          }
        },
        "resourcePropertyCounts": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "propertyName": {
                "type": "string"
              },
              "propertyValue": {
                "type": "string"
              },
              "count": {
                "type": "integer"
              }
            }
          }
        },
        "priority": {
          "type": "integer"
        },
        "escalated": {
          "type": "boolean"
        },
        "wasEscalated": {
          "type": "boolean"
        },
        "correlationDetails": {
          "type": "object",
          "properties": {
            "cluster": {
              "type": "string"
            }
          }
        },
        "accessScopes": {
          "type": "array"
        },
        "assignedBy": {
          "type": "string"
        }
      }
    },
    "raw": {},
    "inputs": {
      "type": "object",
      "properties": {
        "ocm_incident_state": {
          "type": "string"
        },
        "ocm_incident_team": {
          "type": "string"
        },
        "ocm_includecounts": {
          "type": "boolean"
        },
        "ocm_eventsummary": {
          "type": "boolean"
        },
        "ocm_incident_id": {
          "type": "string"
        }
      }
    },
    "metrics": {
      "type": "object",
      "properties": {
        "version": {
          "type": "string"
        },
        "package": {
          "type": "string"
        },
        "package_version": {
          "type": "string"
        },
        "host": {
          "type": "string"
        },
        "execution_time_ms": {
          "type": "integer"
        },
        "timestamp": {
          "type": "string"
        }
      }
    }
  }
}