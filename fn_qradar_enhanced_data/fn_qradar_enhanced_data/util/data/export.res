  "scripts": [
    {
      "actions": [],
      "creator_id": "master@example.com",
      "description": "Create artifact from Assets info for the selected row",
      "export_key": "Create Artifact from Assets info",
      "id": 19,
      "language": "python",
      "last_modified_by": "master@example.com",
      "last_modified_time": 1615261611034,
      "name": "Create Artifact from Assets info",
      "object_type": "qr_assets",
      "script_text": "#\n# We create artifacts according to how they can be mapped to\n# Resilient default artifacts. If user has custom artifacts, and wants\n# to map them as well, please modify the following mapping dict.\n#\n\ntype_mapping = {\n    \"IP Address\": \"IP Address\",\n    \"Name\": \"String\",\n\n}\n\nimport re\n\n\nartifact_types = rule.properties.select_to_create_artifact_from_asset_info\n\nfor type in artifact_types:\n  if type in type_mapping:\n    artifact_description = \"QRadar Offense {0}\".format(type)\n    if type==\"IP Address\":\n     incident.addArtifact(type_mapping[type], row.ip_address[\"content\"], artifact_description)\n    elif type==\"Name\":\n       incident.addArtifact(type_mapping[type], row.asset_name[\"content\"], artifact_description)\n\n",
      "tags": [],
      "uuid": "56272bd6-1c9c-4043-88aa-0b7f1aac382c"
    },
    {
      "actions": [],
      "creator_id": "master@example.com",
      "description": "Create artifact from Destination IP info for the selected row",
      "export_key": "Create Artifact from Destination IP info",
      "id": 20,
      "language": "python",
      "last_modified_by": "master@example.com",
      "last_modified_time": 1615261611135,
      "name": "Create Artifact from Destination IP info",
      "object_type": "qr_top_destination_ips",
      "script_text": "#\n# We create artifacts for those observables according to how they can be mapped to\n# Resilient default artifacts. If user has custom artifacts, and wants\n# to map them as well, please modify the following mapping dict.\n#\n\ntype_mapping = {\n    \"Destination IP\": \"IP Address\",\n}\n\nimport re\n\n\nartifact_types = rule.properties.select_to_create_artifact_from_destip\n\nfor type in artifact_types:\n  if type in type_mapping:\n    artifact_description = \"QRadar Offense {0}\".format(type)\n    if type==\"Destination IP\":\n      incident.addArtifact(type_mapping[type], re.sub(\"\u003c[^\u003c\u003e]+\u003e\",\"\",row.destination_ip[\"content\"]), artifact_description)\n    \n\n",
      "tags": [],
      "uuid": "b6f06359-9d13-4244-bd0f-123dc4068317"
    },
    {
      "actions": [],
