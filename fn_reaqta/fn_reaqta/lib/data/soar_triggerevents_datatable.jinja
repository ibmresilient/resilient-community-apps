[
  {% for event in triggerEvents %}
    {
        "cells": {
            "happened_at": {
                "value": {{ event.happenedAt|soar_datetimeformat(split_at='.') }}
            },
            "category": {
                "value": "{{ event.category }}"
            },
            "relevance": {
                "value": {{ event.relevance }}
            },
            "severity": {
                "value": "{{ event.severity }}"
            },
            "program_path": {
                "value": "{{ event.process.program.path|replace("\\", "\\\\") }}"
            },
            "process_pid": {
                "value": {{ event.process.pid }}
            },
            "sha256_hash": {
                "value": "{{ event.process.program.sha256 }}"
            }
        }
    }
    {%- if not loop.last %},{% endif %}
  {% endfor %}
]