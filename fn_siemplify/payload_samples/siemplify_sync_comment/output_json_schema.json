      "type": "number"
    },
    "success": {
      "type": "boolean"
    },
    "reason": {
      "type": "string"
    },
    "content": {
      "type": "object",
      "properties": {
        "ErrorCode": {
          "type": "integer"
        },
        "ErrorMessage": {
          "type": "string"
        },
        "InnerException": {},
        "InnerExceptionType": {}
      }
