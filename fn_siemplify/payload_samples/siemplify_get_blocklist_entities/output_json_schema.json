{
  "$schema": "http://json-schema.org/draft-06/schema",
  "type": "object",
  "properties": {
    "version": {
      "type": "number"
    },
    "success": {
      "type": "boolean"
    },
    "reason": {},
    "content": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "entityIdentifier": {
            "type": "string"
          },
          "entityType": {
            "type": "string"
          },
          "elementType": {
            "type": "integer"
          },
          "scope": {
            "type": "integer"
          },
          "environments": {
            "type": "array",
            "items": {
              "type": "string"
