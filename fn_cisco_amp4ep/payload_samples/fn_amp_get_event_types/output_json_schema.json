{
  "$schema": "http://json-schema.org/draft-06/schema",
  "type": "object",
  "properties": {
    "response": {
      "type": "object",
      "properties": {
        "version": {
          "type": "string"
        },
        "metadata": {
          "type": "object",
          "properties": {
            "links": {
              "type": "object",
              "properties": {
                "self": {
                  "type": "string"
                }
              }
            },
            "results": {
              "type": "object",
              "properties": {
                "total": {
                  "type": "integer"
                }
              }
            }
          }
        },
        "data": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer"
              },
              "name": {
                "type": "string"
              },
              "description": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "query_execution_time": {
      "type": "string"
    }
  }
}